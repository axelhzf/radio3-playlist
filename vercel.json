{
  "crons": [
    {
      "path": "/cron",
      "schedule": "0 9 * * *"
    }
  ],
  "functions": {
    "api/index.ts": {
      "maxDuration": 300
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/api/index.ts"
    }
  ]
}
